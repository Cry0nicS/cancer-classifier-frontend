{
    "name": "cancer-classifier-frontend",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "clean": "rm -rf node_modules/",
        "dev": "nuxt dev --no-clear",
        "format": "prettier --write \"**/*.{js,json,jsx,scss,ts,tsx,vue}\"",
        "format-check": "prettier --check \"**/*.{html,js,json,jsx,scss,ts,tsx,vue,scss}\"",
        "generate": "nuxt generate",
        "lint": "eslint .",
        "postinstall": "nuxt prepare",
        "prepare": "simple-git-hooks",
        "preview": "nuxt preview",
        "spell-check": "cspell --no-progress",
        "style-check": "stylelint \"**/*.{scss,vue}\"",
        "style-fix": "stylelint --fix \"**/*.{scss,vue}\"",
        "type-check": "vue-tsc --noEmit",
        "ui": "npx --yes ui-thing@latest add"
    },
    "dependencies": {
        "@firebase/app-types": "^0.9.2",
        "@morev/vue-transitions": "^3.0.2",
        "@nuxt/image": "^1.7.0",
        "@vee-validate/nuxt": "^4.13.1",
        "@vee-validate/yup": "^4.13.1",
        "env-var": "^7.5.0",
        "firebase": "^10.12.2",
        "firebase-admin": "^11.11.1",
        "firebase-functions": "^4.9.0",
        "radix-vue": "^1.8.4",
        "sass": "^1.77.6",
        "tailwind-variants": "^0.2.1",
        "typescript": "^5.4.5",
        "ufo": "^1.5.3",
        "vue-sonner": "^1.1.2",
        "yup": "^1.4.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^2.21.1",
        "@cspell/dict-de-de": "^4.0.0",
        "@cspell/dict-lorem-ipsum": "^4.0.0",
        "@nuxt/devtools": "latest",
        "@nuxtjs/color-mode": "^3.4.1",
        "@nuxtjs/i18n": "^8.3.1",
        "@nuxtjs/mdc": "^0.8.2",
        "@nuxtjs/stylelint-module": "^5.2.0",
        "@nuxtjs/tailwindcss": "^6.12.0",
        "@types/node": "^20.14.5",
        "@types/uuid": "^9.0.8",
        "@vueuse/core": "^10.11.0",
        "@vueuse/nuxt": "^10.11.0",
        "autoprefixer": "^10.4.19",
        "cspell": "^8.9.0",
        "eslint": "^9.5.0",
        "eslint-config-prettier": "^9.1.0",
        "lint-staged": "^15.2.7",
        "nuxt": "^3.12.3",
        "nuxt-icon": "^0.6.10",
        "nuxt-vuefire": "^1.0.2",
        "postcss": "^8.4.38",
        "postcss-html": "^1.7.0",
        "postcss-nesting": "^12.1.5",
        "prettier": "3.3.2",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "simple-git-hooks": "^2.11.1",
        "stylelint": "^16.6.1",
        "stylelint-config-standard-scss": "^13.1.0",
        "stylelint-config-standard-vue": "^1.0.0",
        "tailwindcss": "^3.4.3",
        "tailwindcss-animate": "^1.0.7",
        "vue-tsc": "^2.0.24"
    },
    "simple-git-hooks": {
        "commit-msg": "npx commitlint --edit \"$1\"",
        "pre-commit": "npx lint-staged"
    },
    "lint-staged": {
        "*.{html,js,json,md,scss,ts,vue}": "cspell --no-must-find-files",
        "*.{html,js,json,scss,ts,vue}": "prettier --check",
        "*.{js,ts,vue}": "eslint",
        "*.{scss,vue}": "stylelint"
    }
}
