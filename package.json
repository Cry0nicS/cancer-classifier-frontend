{
    "name": "cancer-classifier-frontend",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "clean": "rm -rf node_modules/",
        "dev": "nuxt dev --no-clear",
        "format": "prettier --write \"**/*.{js,json,jsx,scss,ts,tsx,vue}\"",
        "format-check": "prettier --check \"**/*.{html,js,json,jsx,scss,ts,tsx,vue,scss}\"",
        "generate": "nuxt generate",
        "lint": "eslint .",
        "postinstall": "nuxt prepare",
        "prepare": "simple-git-hooks",
        "preview": "nuxt preview",
        "spell-check": "cspell --no-progress",
        "style-check": "stylelint \"**/*.{scss,vue}\"",
        "style-fix": "stylelint --fix \"**/*.{scss,vue}\"",
        "type-check": "vue-tsc --noEmit"
    },
    "dependencies": {
        "@heroicons/vue": "^2.1.1",
        "@nuxt/image": "^1.0.0",
        "sass": "^1.69.4",
        "swiper": "^11.0.4",
        "typescript": "^5.2.2",
        "ufo": "^1.5.3",
        "uuid": "^9.0.1",
        "vue-matomo": "^4.2.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^1.0.0-beta.28",
        "@cspell/dict-de-de": "^3.1.1",
        "@cspell/dict-lorem-ipsum": "^4.0.0",
        "@nuxt/devtools": "latest",
        "@nuxtjs/i18n": "^8.0.0",
        "@nuxtjs/stylelint-module": "^5.1.0",
        "@types/node": "^20.8.8",
        "@types/swiper": "^6.0.0",
        "@types/uuid": "^9.0.7",
        "@vueuse/core": "^10.6.1",
        "@vueuse/nuxt": "^10.6.1",
        "autoprefixer": "^10.4.16",
        "cspell": "^7.3.8",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "lint-staged": "^15.0.2",
        "nuxt": "^3.8.0",
        "postcss": "^8.4.31",
        "postcss-html": "^1.5.0",
        "postcss-nesting": "^12.0.1",
        "prettier": "3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.6",
        "simple-git-hooks": "^2.9.0",
        "stylelint": "^15.11.0",
        "stylelint-config-standard-scss": "^11.0.0",
        "stylelint-config-standard-vue": "^1.0.0",
        "tailwindcss": "^3.3.4",
        "vue-tsc": "^1.8.20"
    },
    "simple-git-hooks": {
        "commit-msg": "npx commitlint --edit \"$1\"",
        "pre-commit": "npx lint-staged"
    },
    "lint-staged": {
        "*.{html,js,json,md,scss,ts,vue}": "cspell --no-must-find-files",
        "*.{html,js,json,scss,ts,vue}": "prettier --check",
        "*.{js,ts,vue}": "eslint",
        "*.{scss,vue}": "stylelint"
    }
}
