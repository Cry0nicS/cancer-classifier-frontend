{
    "name": "cancer-classifier-frontend",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "clean": "rm package-lock.json && rm -rf node_modules/ && npx nuxi cleanup",
        "dev": "nuxt dev --no-clear",
        "format": "prettier --write \"**/*.{js,json,jsx,scss,ts,tsx,vue}\"",
        "format-check": "prettier --check \"**/*.{html,js,json,jsx,scss,ts,tsx,vue,scss}\"",
        "generate": "nuxt generate",
        "lint": "eslint .",
        "postinstall": "nuxt prepare",
        "prepare": "simple-git-hooks",
        "preview": "nuxt preview",
        "spell-check": "cspell --no-progress",
        "style-check": "stylelint \"**/*.{scss,vue}\"",
        "style-fix": "stylelint --fix \"**/*.{scss,vue}\"",
        "type-check": "vue-tsc --noEmit",
        "ui": "npx --yes ui-thing@latest add"
    },
    "dependencies": {
        "@firebase/app-types": "^0.9.3",
        "@morev/vue-transitions": "^3.0.5",
        "@nuxt/eslint": "^0.7.6",
        "@nuxt/image": "^1.11.0",
        "@vee-validate/nuxt": "^4.15.1",
        "@vee-validate/yup": "^4.15.1",
        "@vitejs/plugin-vue": "^5.2.4",
        "@vue-email/components": "^0.0.21",
        "@vue-email/render": "^0.0.9",
        "env-var": "^7.5.0",
        "firebase": "^10.14.1",
        "firebase-admin": "^12.7.0",
        "firebase-functions": "^5.1.1",
        "lodash-es": "^4.17.21",
        "nodemailer": "^6.10.1",
        "radix-vue": "^1.9.17",
        "sass": "^1.96.0",
        "tailwind-variants": "^0.3.1",
        "typescript": "^5.9.3",
        "ufo": "^1.6.1",
        "vue-sonner": "^1.3.2",
        "yup": "^1.7.1"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^6.2.0",
        "@commitlint/cli": "^19.8.1",
        "@cspell/dict-de-de": "^4.1.2",
        "@cspell/dict-lorem-ipsum": "^4.0.5",
        "@nuxt/content": "^2.13.4",
        "@nuxt/devtools": "latest",
        "@nuxtjs/color-mode": "^3.5.2",
        "@nuxtjs/i18n": "^8.5.6",
        "@nuxtjs/mdc": "^0.19.1",
        "@nuxtjs/robots": "^4.1.11",
        "@nuxtjs/stylelint-module": "^5.2.1",
        "@nuxtjs/tailwindcss": "^6.14.0",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.19.2",
        "@types/nodemailer": "^6.4.21",
        "@types/uuid": "^10.0.0",
        "@vueuse/core": "^11.3.0",
        "@vueuse/nuxt": "^11.3.0",
        "autoprefixer": "^10.4.22",
        "cspell": "^8.19.4",
        "eslint": "^9.39.2",
        "lint-staged": "^15.5.2",
        "nuxt": "^3.20.2",
        "nuxt-icon": "^0.6.10",
        "nuxt-rollbar": "^0.2.2",
        "nuxt-vuefire": "^1.1.0",
        "postcss": "^8.5.6",
        "postcss-html": "^1.8.0",
        "postcss-nesting": "^13.0.2",
        "prettier": "^3.7.4",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "simple-git-hooks": "^2.13.1",
        "stylelint": "^16.26.1",
        "stylelint-config-standard-scss": "^13.1.0",
        "stylelint-config-standard-vue": "^1.0.0",
        "tailwindcss": "^3.4.19",
        "tailwindcss-animate": "^1.0.7",
        "vue-tsc": "^2.2.12"
    },
    "simple-git-hooks": {
        "commit-msg": "npx commitlint --edit \"$1\"",
        "pre-commit": "npx lint-staged"
    },
    "lint-staged": {
        "*.{html,js,json,md,scss,ts,vue}": "cspell --no-must-find-files",
        "*.{html,js,json,scss,ts,vue}": "prettier --check",
        "*.{js,ts,vue}": "eslint",
        "*.{scss,vue}": "stylelint"
    }
}
